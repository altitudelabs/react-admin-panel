{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "boilerplate for universal react project with redux and babel",
  "main": "index.js",
  "scripts": {
    "reset": "npm uninstall react-admin-panel && npm install",
    "deploy-gh": "webpack --config webpack.gh.config.js && node ./gh-pages/deploy",
    "build:watch": "webpack-dev-server --config webpack.config.js --inline",
    "build": "NODE_ENV=production webpack",
    "test": ""
  },
  "engines": {
    "node": "^6.3.0",
    "npm": "^3.10.3"
  },
  "author": "Altitude Labs",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "qs": "^6.4.0",
    "react": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-modal": "^1.7.3",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.3",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.22.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
